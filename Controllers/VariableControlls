@foreach (var Contact in Model)
{
  <h3> @Contact.firstName() @Contact.lastName() </h3>
  <p> @Contact.number() </p>
  <p> @Contact.email() </p>
  <p> @Contact.address() </p>
}

@foreach (var Address in Model)
{
  <p>
  @Address.streetNum()
  @Address.streetName()
  </p>
  <p>
  @Address.city()
  @Address.state()
  @Address.zipcode()
  </p>
}
